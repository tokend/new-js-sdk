{
  "name": "@tokend/js-sdk",
  "version": "1.0.0-x.8",
  "description": "js-sdk is a client-side SDK for TokenD asset tokenization platform.",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src -d lib --ignore spec.js,test_helpers",
    "test": "mocha --opts ./mocha.opts 'src/**/*.spec.js'",
    "test:integration": "mocha --timeout 10000000 --opts ./mocha.opts 'test/**/*.spec.js'",
    "build:browser": "rimraf dist && webpack --ignore spec.js,test_helpers",
    "test:browser": "karma start",
    "lint": "eslint ./src --cache --fix",
    "coverage": "nyc mocha --opts ./mocha.opts 'src/**/*.spec.js'",
    "docs": "rimraf docs/pages && npm run docs:xdr && jsdoc -c ./.jsdoc.json && rm docs/enums.js",
    "docs:md": "rimraf docs/markdown && node scripts/jsdoc_to_markdown.js",
    "docs:xdr": "node scripts/xdr_to_jsdoc.js",
    "prepare": "npm run build & npm run build:browser",
    "init:sales": "node scripts/init_sales.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint ./src --cache"
    }
  },
  "nyc": {
    "exclude": [
      "src/**/*.spec.js",
      "src/test_helpers",
      "src/base/generated"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "tokend"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/tokend/new-js-sdk.git"
  },
  "author": "Distributed Lab <contact@distributedlab.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://gitlab.com/tokend/new-js-sdk.git/issues"
  },
  "homepage": "https://gitlab.com/tokend/new-js-sdk",
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.1.2",
    "@babel/runtime-corejs2": "7.1.2",
    "axios-mock-adapter": "1.15.0",
    "babel-loader": "8.0.4",
    "babel-plugin-transform-builtin-extend": "1.1.2",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-json-equal": "0.0.1",
    "eslint": "4.19.1",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-chai-friendly": "0.4.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "3.1.0",
    "husky": "0.15.0-rc.13",
    "jsdoc": "3.5.5",
    "jsdoc-to-markdown": "4.0.1",
    "karma": "2.0.5",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-mocha": "1.3.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sinon": "1.0.5",
    "karma-webpack": "3.0.5",
    "minami": "1.2.3",
    "mocha": "5.2.0",
    "nyc": "11.9.0",
    "sinon": "6.3.5",
    "sinon-chai": "3.2.0",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.2",
    "chalk": "2.4.1"
  },
  "dependencies": {
    "axios": "0.16.2",
    "base32.js": "0.1.0",
    "bignumber.js": "2.4.0",
    "crc": "3.8.0",
    "ed25519": "0.0.4",
    "js-xdr": "1.0.5",
    "lodash": "4.17.11",
    "sjcl-tokend": "1.0.6",
    "tweetnacl": "0.13.3",
    "urijs": "1.19.1",
    "jsona": "1.3.0",
    "loglevel": "1.6.1",
    "loglevel-plugin-prefix": "0.8.4",
    "moment": "2.24.0"
  },
  "optionalDependencies": {
    "ed25519": "0.0.4"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib"
  }
}
